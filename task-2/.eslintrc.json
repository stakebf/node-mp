{
    "root": true,
    "parserOptions": {
         "ecmaVersion": "latest",
         "sourceType": "module"
    },
    "extends": ["eslint:recommended"],
    "env": { 
         "es2021": true,
         "node": true
    },
    "rules": {
        "indent": ["error", 2],
        "quotes": ["error","single"],
        "max-len": ["error", { "code": 100 }]
    }
}
